<Window x:Class="LeftSide.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Left Side" Height="350" Width="525">
    <Grid>
        <Button HorizontalAlignment="Left" Margin="41,26,0,0" VerticalAlignment="Top" Width="100" Height="56" Click="GeneratePublicPrivateKey">
            <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                Generate Public/Private Key Pair
            </TextBlock>
        </Button>
        <Button HorizontalAlignment="Left" Margin="177,26,0,0" VerticalAlignment="Top" Width="100" Height="56" Click="DecryptSymetricKey">
            <TextBlock TextWrapping="Wrap" TextAlignment="Center">
                Decrypt Symetric Key
            </TextBlock>
        </Button>
        <Button Name="ReadButton" Content="Read" HorizontalAlignment="Left" Margin="41,106,0,0" VerticalAlignment="Top" Width="75" Click="ReadButton_Click"/>
        <Button Name="WriteButton" Content="Write" HorizontalAlignment="Left" Margin="41,146,0,0" VerticalAlignment="Top" Width="75" Click="WriteButton_Click"/>
        <TextBox Name="messageTextbox" HorizontalAlignment="Left" Height="207" Margin="145,103,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="362"/>

    </Grid>
</Window>
